   -ms-overflow-style: none;  /* Internet Explorer 10+ */\n    scrollbar-width: none;  /* Firefox */\n}\n\n.skills-container-a::-webkit-scrollbar {\n    display: none;  /* Para Chrome, Safari y Opera */\n}\n\n.bulto{\n    height: 200vh;\n}\n.skills-container {\n    height: 100%;\n    margin-bottom: 200vh;\n    position: fixed;\n    top: 6.5%;\n    left: 35%;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    gap: 50px;\n    perspective: 1000px;\n    z-index: 1;\n}\n\n.progress-line {\n    display: flex;\n    position: relative;\n    top: -118px;\n    height: 110%;\n    width: 293px;\n    background-color: #ddd;\n    transform: rotateX(60deg);\n    justify-content: center;\n    overflow: hidden;\n    flex-wrap: nowrap;\n    align-content: center;\n}\n\n.container-line-three{\n    position: relative;\n    display: flex;\n    flex-direction: column;\n    flex-wrap: nowrap;\n    justify-content: space-around;\n    transform: translateY(-1000px); \n    left: 17px;\n    \n}\n\n\n.container-line-two{\n    position: relative;\n    display: flex;\n    flex-direction: column;\n    flex-wrap: nowrap;\n    justify-content: space-around;\n    transform: translateY(-1000px); \n    left: 7px;\n    \n}\n\n\n.container-line-one{\n    position: relative;\n    display: flex;\n    flex-direction: column;\n    flex-wrap: nowrap;\n    justify-content: space-around;\n    transform: translateY(-1000px);\n    left: -3px;\n}\n\n\n\n\n.line {\n    \n    height: 50px;\n    width: 2%;\n    bottom: 0;\n    \n    transition: height 0.3s ease;\n    transform: scale(1) translateX(-2px) translateY(-9px);\n    border: solid black; \n    border-width: 5px;\n}\n\n.dot {\n    position: absolute;\n    height: 20px;\n    width: 20px;\n    background-color: #fff;\n    border: 3px solid #0d6efd;\n    border-radius: 50%;\n    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.3); /* Sombra suave */\n}\n\n#dot1 {\n    top: 20px;\n}\n\n#dot2 {\n    top: 160px;\n}\n\n#dot3 {\n    top: 280px;\n}\n\n.skills {\n    display: flex;\n    flex-direction: column;\n    gap: 27px;\n    transform: rotateX(0deg) rotateY(357deg) rotateX(39deg);\n    position: relative;\n    top: -62px;\n    height: 100%;\n    left: -36px;\n}\n\n.skill h3 {\n    margin-bottom: 10px;\n    color: #0d6efd;\n    font-weight: bold; /* Hacer el texto más destacado */\n}\n.skill {\n    background-color: #fff;\n    border-radius: 10px;\n    padding: 20px;\n    width: 200px;\n    box-shadow: 0 4px 20px rgba(0, 0, 0, 0.4); /* Sombra más intensa */\n    opacity: 0;\n    transform: translateX(-40px) translateZ(0) rotateY(5deg) rotateX(2deg); /* Suave efecto 3D */\n    transform-origin: center; /* Cambiar el origen para centrado */\n    transition: opacity 0.3s ease, transform 0.6s ease; /* Transición suave */\n}\n\n.skill.active {\n    opacity: 1;\n    transform: translateX(0) scale(1.02) rotateY(0deg) rotateX(0deg) rotate(45deg);\n    transition: opacity 0.3s ease, transform 0.6s ease;\n}\n\n/* Añadir un hover effect para mejorar la interacción */\n.skill:hover {\n    box-shadow: 0 8px 25px rgba(0, 0, 0, 0.5);\n    transform: translateX(0) scale(1.50) rotateY(0deg) rotateZ(0deg) translate(32%, -28%);\n    top: 50%;\n    left: 50%;\n    cursor: pointer;\n}\n\n.car {\n    height: 50px;\n    width: 50px;\n    position: relative;\n    top: 234px;\n    z-index: 1;\n    \n    transform: rotateX(45deg) rotateY(5deg) translateZ(-5px);\n    filter: drop-shadow(5px 5px 10px rgba(0, 0, 0, 0.5)); /* Sombra siguiendo la forma del coche */\n    transition: transform 0.5s ease, filter 0.5s ease;\n}\n\n"},"lineCount":171}},"error":null,"hash":"2dc7fb8a937e9c25c02704b96d1a27ec","cacheData":{"env":{}}}